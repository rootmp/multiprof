	<skill id="47919" levels="13" name="Summoner's Armor Mastery">
		<!--M. Def. $s2\nM. Damage Resistance $s3\n\nWhen using robes:\nP. Def. $s1\nMax HP $s4\n\nWhen using light armor:\nP. Def. $s5\nCasting Spd. $s6\nAtk. Spd. $s7\nMP Recovery Rate $s8-->
		<table name="#mDef">30 50 80 110 120 125 130 135 140 150 160 170 180</table>
		<table name="#receive_m_damage_power">1.15 1.15 1.2 1.3 1.3 1.3 1.3 1.3 1.3 1.3 1.3 1.3 1.3</table>
		<!--Magic armor-->
		<table name="#pDef_Magic">30 40 50 65 73 80 85 90 96 102 110 115 120</table>
		<table name="#maxHp_Magic">60 90 140 220 250 270 290 310 330 350 370 390 410</table>
		<!--Light armor-->
		<table name="#pDef_Light">13 21 30 41 48 52 56 60 64 69 75 80 90</table>
		<table name="#mAtkSpd_Light">1.88 1.88 1.88 1.88 1.88 1.88 1.88 1.88 1.88 1.88 1.90 1.90 1.90</table>
		<table name="#pAtkSpd_Light">1.25</table>
		<table name="#regMp_Light">1.2</table>
		<table name="#magicLevel">20 30 40 50 60 70 75 80 85</table>
		<set name="icon" value="icon.s_sum_arm_mastery"/>
		<set name="magicLevel" value="#magicLevel"/>
		<set name="operate_type" value="P"/>
		<for>
			<add stat="mDef" value="#mDef"/>
			<add stat="receive_m_damage_power" value="#receive_m_damage_power"/>
			<!--Magic armor-->
			<add stat="pDef" value="#pDef_Magic">
				<using armor="Magic"/>
			</add>
			<add stat="maxHp" value="#maxHp_Magic">
				<using armor="Magic"/>
			</add>
			<!--Light armor-->
			<add stat="pDef" value="#pDef_Light">
				<using armor="Light"/>
			</add>
			<mul stat="mAtkSpd" value="#mAtkSpd_Light">
				<using armor="Light"/>
			</mul>
			<mul stat="pAtkSpd" value="#pAtkSpd_Light">
				<using armor="Light"/>
			</mul>
			<mul stat="regMp" value="#regMp_Light">
				<using armor="Light"/>
			</mul>
		</for>
